{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "cmake.configureSettings": {
        "SE_USE_COMPRESSINATOR":"ON",
        "SE_NAVIGATION":"OFF",
        "SE_HAS_ASSIMP":"ON",
        "SE_HAS_EDITOR":"ON"
    },
    "cmake.buildDirectory": "${workspaceFolder}/build/${buildType}",
    "search.exclude": {
        "**/build": true,
        "**/GameBuild": true,
        "**/assets": true,
        "**/SAVES": true,
        "**/temp": true
    },
    "files.associations": {
        "*.asset": "yaml",
        "vector": "cpp",
        "algorithm": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
        "cinttypes": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "deque": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "functional": "cpp",
        "initializer_list": "cpp",
        "iomanip": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "iterator": "cpp",
        "limits": "cpp",
        "list": "cpp",
        "locale": "cpp",
        "map": "cpp",
        "memory": "cpp",
        "new": "cpp",
        "optional": "cpp",
        "ostream": "cpp",
        "set": "cpp",
        "sstream": "cpp",
        "stack": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "utility": "cpp",
        "xfacet": "cpp",
        "xhash": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocbuf": "cpp",
        "xlocinfo": "cpp",
        "xlocmes": "cpp",
        "xlocmon": "cpp",
        "xlocnum": "cpp",
        "xloctime": "cpp",
        "xmemory": "cpp",
        "xstddef": "cpp",
        "xstring": "cpp",
        "xtr1common": "cpp",
        "xtree": "cpp",
        "xutility": "cpp",
        "hash_map": "cpp",
        "hash_set": "cpp",
        "*.tcc": "cpp",
        "forward_list": "cpp",
        "valarray": "cpp",
        "regex": "cpp",
        "chrono": "cpp",
        "memory_resource": "cpp",
        "scoped_allocator": "cpp",
        "complex": "cpp",
        "string_view": "cpp",
        "stop_token": "cpp",
        "shared_mutex": "cpp",
        "*.def": "cpp",
        "any": "cpp",
        "bitset": "cpp",
        "future": "cpp",
        "variant": "cpp",
        "typeindex": "cpp",
        "semaphore": "cpp",
        "random": "cpp",
        "*.inc": "cpp",
        "mutex": "cpp",
        "codecvt": "cpp",
        "condition_variable": "cpp",
        "csignal": "cpp",
        "cwctype": "cpp",
        "numeric": "cpp",
        "ratio": "cpp",
        "source_location": "cpp",
        "numbers": "cpp",
        "thread": "cpp",
        "cfenv": "cpp",
        "*.moc": "cpp",
        "span": "cpp"
    },
    "Lua.diagnostics.globals": [
        "AssetDatabase",
        "Instantiate",
        "Mathf",
        "vector",
        "GridSystem",
        "Time",
        "Quaternion",
        "Vector2Int",
        "Vector2",
        "LogError",
        "continue",
        "SceneManager",
        "ButterflyGame",
        "Vector3",
        "imgui",
        "LogWarning",
        "length",
        "Grid"
    ],
    "robloxLsp.diagnostics.globals": [
        "AssetDatabase"
    ],
    "robloxLsp.develop.enable": true,
    "Lua.misc.parameters": [
        "--develop=true"
    ],
    "Lua.runtime.plugin": ".vscode/lua/plugin.lua",
    "Lua.workspace.library": [
        "engine\\assets\\scripts"
    ],
    "lldb.showDisassembly": "auto",
    "lldb.dereferencePointers": true,
    "lldb.consoleMode": "commands"
}