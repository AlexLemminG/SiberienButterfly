GameObject:
  tag: Grid
  components:
    - $Transform
    - $GridDrawer
    - $GridCollider
    - $RigidBody
    - $Grid
Transform:
  pos:
    - 0
    - 0
    - 0
  euler:
    - 0
    - -0
    - 0
  scale:
    - 1
    - 1
    - 1
GridDrawer:
  gridCellPrefab: prefabs/gridCell.asset
  useFrustumCulling: true
  castsShadows: true
Grid$Grid: ~
GridCollider:
  chunkPrefab: prefabs/gridChunkCollider.asset$GameObject
RigidBody:
  isStatic: 1
  isKinematic: false
