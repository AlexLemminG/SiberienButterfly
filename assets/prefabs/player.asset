GameObject:
  tag: player
  components:
    - $Transform
    - $MeshRenderer
    - $PlayerController
    - $CapsuleCollider
    - $RigidBody
    - $Animator
Transform:
  scale:
    - 1
    - 1
    - 1
MeshRenderer:
  mesh: models\Vintik.blend$Mesh
  material: $M1
Material$M1:
  shader: engine\shaders\main.asset
  colorTex: engine\textures\white.png
PlayerController:
  speed: 9.0
  speedWithItem: 4.5
  runAnimation: models\Vintik.blend$Run
  standAnimation: models\Vintik.blend$Stand
  runAnimationWithItem: models\Vintik.blend$RunWithItem
  standAnimationWithItem: models\Vintik.blend$StandWithItem
CapsuleCollider:
  radius: 0.34
  center:
    - 0
    - 1.02
    - 0
  height: 1.4869586
RigidBody:
  isStatic: false
  isKinematic: false
  restitution: 0.0
  friction: 0.0
  mass: 25
  layer: player
Animator:
  speed: 1
  defaultAnimation: models\Vintik.blend$Stand
