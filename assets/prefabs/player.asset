GameObject:
  tag: player
  components:
  - $Transform
  - $MeshRenderer
  - $PlayerController
  - $SphereCollider
  - $RigidBody

Transform:
  scale: [0.7,1.5,0.7]
  
MeshRenderer:
  mesh: engine\models\Cube.glb$Mesh
  material: $M1
  
Material$M1:
  shader: engine\shaders\main.asset
  colorTex: engine\textures\white.png
  
PlayerController:
    speed: 10
SphereCollider:
  radius: 0.5
  center: [0, 0.0, 0]

RigidBody:
  isStatic: false
  isKinematic: false
  restitution: 0.0
  friction: 0.0
  mass: 50
  layer: player