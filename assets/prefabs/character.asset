GameObject:
  tag: character
  components:
    - $Transform
    - $MeshRenderer
    - $CapsuleCollider
    - $RigidBody
    - $Animator
    - $LuaComponent
Transform:
  scale:
    - 1
    - 1
    - 1
MeshRenderer:
  mesh: models\Vintik.blend$Mesh
  material: $M1
Material$M1:
  shader: engine\shaders\main.asset
  colorTex: engine\textures\white.png
CapsuleCollider:
  radius: 0.34
  center:
    - 0
    - 1.02
    - 0
