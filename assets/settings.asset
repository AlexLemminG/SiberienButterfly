PhysicsSettings:
  layers:
    - name: default
      doNotCollideWith:
        - playerBullet
    - name: player
      doNotCollideWith:
        - playerBullet
        - enemyCorpse
        - grenade
    - name: enemy
      doNotCollideWith:
        - grenade
    - name: playerBullet
      doNotCollideWith:
        - player
        - default
        - grenade
    - name: enemyCorpse
      collideWith:
        - all
    - name: grenade
      doNotCollideWith:
        - default
        - player
        - enemy
        - playerBullet
    - name: staticGeom
      doNotCollideWith:
        - staticGeom
ShadowSettings:
  far: 30
  stabilize: 1
  near_far: 100
  near: 1
  pcfOffset: 0.5
TimeSettings:
  timeScale: 1
RenderSettings:
  backend: 2
